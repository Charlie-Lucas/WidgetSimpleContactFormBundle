    "type"             : "symfony-bundle",
    "description"      : "Victoire DCMS SimpleContactForm widget",
    "keywords"         : [
        "DCMS",
        "victoire",
        "SimpleContactForm",
        "widget",
        "FriendsofVictoire",
        "FoV",
        "Dynamic",
        "CMS"
    ],
    "homepage"         : "http://victoire.io",
