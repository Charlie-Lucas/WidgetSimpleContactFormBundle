{
    "name"             : "friendsofvictoire/simplecontactform-widget",
    "type"             : "symfony-bundle",
    "description"      : "Victoire DCMS SimpleContactForm widget",
    "keywords"         : [
        "DCMS",
        "victoire",
        "SimpleContactForm",
        "widget",
        "FriendsofVictoire",
        "FoV",
        "Dynamic",
        "CMS"
    ],
    "homepage"         : "http://victoire.io",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Leny BERNARD",
            "email": "leny@victoire.io"
        }
    ],
    "require": {
        "php"                           : ">=5.3.3",
        "symfony/framework-bundle"      : "~2.3",
        "victoire/victoire"             : "~1.3",
        "appventus/shortcuts-bundle"    : "dev-master",
        "appventus/swiftmailerdbbundle" : "dev-master",
        "appventus/spoolmailerbundle"   : "dev-master"
    },
    "autoload"         : {
        "psr-0": {"Victoire\\Widget\\SimpleContactFormBundle": ""}
